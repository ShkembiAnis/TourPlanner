<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
      GridPane.rowIndex="2" GridPane.rowSpan="6" GridPane.columnIndex="1"
      GridPane.columnSpan="1" minHeight="400"
      fx:controller="at.technikum_wien.tourplanner_anis_mariel.tourAdd.TourAddController"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://javafx.com/fxml ">
    <HBox>
        <Label minWidth="-Infinity" text="Tours:  "></Label>
        <StackPane>
            <TextField fx:id="tourName"></TextField>
        </StackPane>
        <StackPane>
            <Button fx:id="buttonAddTour" text="+" styleClass="mainButton"/>
        </StackPane>
    </HBox>
    <fx:include source="tourList.fxml"/>
</VBox>

